{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "ddt-piano",
	"version": "1.0.11",
	"identifier": "net.d3fau1t.ddt-piano",
	"build": {
		"devUrl": "http://localhost:5173",
		"frontendDist": "../build",
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "두두타 피아노",
				"width": 600,
				"height": 600,
				"resizable": true,
				"minimizable": true,
				"closable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"plugins": {
		"dialog": null,
		"fs": null
	}
}
